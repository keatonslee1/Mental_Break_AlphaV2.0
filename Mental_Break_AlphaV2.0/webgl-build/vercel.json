{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/Build/(.*\\.wasm)",
      "headers": {
        "Content-Type": "application/wasm",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.data)",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.js)",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.wasm\\.gz)",
      "dest": "/Build/$1",
      "headers": {
        "Content-Type": "application/wasm",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.data\\.gz)",
      "dest": "/Build/$1",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.js\\.gz)",
      "dest": "/Build/$1",
      "headers": {
        "Content-Type": "application/javascript",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*)",
      "dest": "/Build/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/TemplateData/(.*)",
      "dest": "/TemplateData/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.framework\\.js\\.unityweb)",
      "headers": {
        "Content-Type": "application/javascript",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.wasm\\.unityweb)",
      "headers": {
        "Content-Type": "application/wasm",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/Build/(.*\\.data\\.unityweb)",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.unityweb)",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Encoding": "gzip",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.wasm)",
      "headers": {
        "Content-Type": "application/wasm",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.js)",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.css)",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.woff2)",
      "headers": {
        "Content-Type": "font/woff2",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.woff)",
      "headers": {
        "Content-Type": "font/woff",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.ttf)",
      "headers": {
        "Content-Type": "font/ttf",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.eot)",
      "headers": {
        "Content-Type": "application/vnd.ms-fontobject",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.svg)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/about",
      "dest": "/about.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/blog",
      "dest": "/blog.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/about.html",
      "dest": "/about.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/blog.html",
      "dest": "/blog.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}

